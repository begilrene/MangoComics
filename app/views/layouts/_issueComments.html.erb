   <% provide(:title, @iss.issueID) %>
    <div class = "container">
      <div class = "row">
        <div id: "comments">
          <div class="col-sm-12">
            <% @iss.IComments.each do |iComments| %>
            
            <p> <%=User.find(iComments.user_id).displayName%> <img src=<%=User.find(iComments.user_id).profilePic%> width="80" height="80" alt=""></p>
